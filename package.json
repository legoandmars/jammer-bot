{
    "name": "jammer-bot",
    "version": "1.0.0",
    "description": "Jammer Discord Bot",
    "author": "legoandmars",
    "private": true,
    "main": "src/index.ts",
    "license": "MIT",
    "engines": {
        "node": ">=12"
    },
    "scripts": {
        "start:dev": "ts-node-dev src/index.ts",
        "start:prod": "ts-node --transpile-only src/index.ts",
        "db:migrate": "prisma migrate up --experimental",
        "db:generate": "prisma generate",
        "lint:fix": "eslint --fix **/*.{ts,js} && prettier --write **/*.{json,md,yml}",
        "lint:check": "eslint **/*.{ts,js} && prettier --check **/*.{json,md,yml}"
    },
    "dependencies": {
        "@prisma/client": "^2.11.0",
        "date-fns": "^2.16.1",
        "discord.js": "^12.4.1",
        "dotenv": "^8.2.0",
        "spotify-web-api-node": "^5.0.0",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.5"
    },
    "devDependencies": {
        "@prisma/cli": "^2.11.0",
        "@types/node": "*",
        "@types/spotify-web-api-node": "^4.0.2",
        "@typescript-eslint/eslint-plugin": "^4.8.1",
        "@typescript-eslint/parser": "^4.8.1",
        "eslint": "^7.13.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-unicorn": "^23.0.0",
        "prettier": "^2.1.2",
        "ts-node-dev": "^1.0.0"
    }
}
